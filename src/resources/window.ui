                          <object class="GtkBox"><property name="spacing">12</property><child><object class="GtkLabel"><property name="label">Отключить обновление драйверов через центр обновления</property><property name="hexpand">true</property><property name="xalign">0</property></object></child><child><object class="GtkSwitch" id="disable_driver_updates_switch"/></child></object>
